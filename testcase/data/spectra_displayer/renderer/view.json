              "script": [
                "let files=[];\n\nlet {\n    username,\n    password\n} = API.getData('prefs').resurrect();\n\nlet headers = new Headers();\nheaders.set('Authorization', 'Basic ' + btoa(username + \":\" + password));\n\n\nlet toc=await getJSON('https://api.github.com/repos/NPellet/jsGraph/contents/examples/v2?ref=2.1');\nfor (let item of toc) {\n    if (item.type==='dir') {\n        let dir=(await getJSON(item.git_url)).tree;\n        for (let file of dir) {\n            console.log(file);\n            if (file.path.endsWith('.json')) {\n                files.push({\n                    directory: item.name,\n                    name: file.path,\n                    dataURL: file.url\n                })\n            }\n        }\n    }\n}\n\nAPI.createData('files', files);\n\nasync function getJSON(url) {\n    let response=await fetch(url,{headers});\n    return response.json();\n}\n\n\n\n\n\n"
