        "filters": [
          {
            "sections": {},
            "groups": {
              "filter": [
                {
                  "name": [
                    "mfToChart"
                  ],
                  "script": [
                    "return {\n    filter: function filter(value, resolve) {\n        var options = API.getData(\"options\").resurrect();\n\n        if (! options.fwhm && options.mass && options.resolution) {\n            options.fwhm=options.mass/options.resolution;\n        }\n\n        var analyseOptions = {\n            typedResult: true,\n            isotopomers: 'array',\n            resolution: options.fwhm\n        };\n        \n        if(options.gaussian) {\n            analyseOptions.gaussianWidth = 10;\n        }\n        \n        var result = CC.analyseMF(value.value, analyseOptions)\n        \n        var spectrum={\n            type:\"chart\",\n            value: {\n                data:[\n                    {\n                        x:result.spectrum[0],\n                        y:result.spectrum[1]\n                    }\n                ]\n            }\n        };\n        resolve(spectrum);\n    }\n}"
                  ]
                }
              ],
              "libs": [
                [
                  {
                    "lib": "CC",
                    "alias": "CC"
                  }
                ]
              ]
            }
          },
          {
            "sections": {},
            "groups": {
              "filter": [
                {
                  "name": [
                    "tabFileParser"
                  ],
                  "script": [
                    "return {\n    filter: function filter(value, resolve) {\n        var realValue=value.get();\n        var lines=realValue.split(/[\\r\\n]+/);\n        var maxY=Number.MIN_VALUE;\n        var result=[];\n\n        for (var i=0; i<lines.length; i++) {\n            var line=lines[i];\n            // we will consider only lines that contains only numbers\n            if (line.match(/^[0-9eE,;\\. \\t]+$/)) {\n                var fields=line.split(/[,; \\t]+/);\n                \n                if (fields && fields.length==2) {\n                    var x=parseFloat(fields[0]);\n                    var y=parseFloat(fields[1]);\n                    if (y>maxY) maxY=y;\n                    result.push([x,y]);\n                }\n            }\n        }\n        maxY/=100;\n        for (var i=0; i<result.length; i++) {\n            result[i][1]/=maxY;\n        }\n\n        // The returned value must be a DataObject.\n        // Use DataObject.check(toReturn, true) to create it for a new variable.\n        resolve(result);\n    }\n}"
                  ]
                }
              ],
              "libs": [
                [
                  {}
                ]
              ]
            }
          }
        ]
      },
      "groups": {}
    }
