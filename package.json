    "Luc Patiny",
    "MichaÃ«l Zasso",
    "Daniel Kostro"
  ],
  "main": "src/init.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/NPellet/visualizer.git"
  },
  "devDependencies": {
    "add-stream": "^1.0.0",
    "bower": "1.4.1",
    "conventional-changelog": "^0.2.1",
    "eslint": "^1.4.3",
    "extend": "^2.0.0",
    "grunt": "~0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-requirejs": "~0.4.4",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-ftp": "^0.1.0",
    "grunt-jsdoc": "^0.5.7",
    "grunt-rename": "^0.1.4",
    "grunt-text-replace": "~0.3.10",
